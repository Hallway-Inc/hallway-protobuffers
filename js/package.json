{
  "name": "@quarkworks-inc/hallway-protobuffer",
  "version": "0.0.1",
  "description": "Hallway Protobuffer for Typescript",
  "license": "SEE LICENSE IN LICENSE.md",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "bugs": {
    "url": ""
  },
  "homepage": "https://github.com/Hallway-Inc/AvatarWebKit#-joinhallwaycom",
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "start": "rm -rf dist && rollup -cw",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings=0 --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "release": "release-it",
    "prepare": "cd .. && husky install js/.husky"
  },
  "dependencies": {
    "protobufjs": "~6.11.2"
  },
  "devDependencies": {
    "husky": ">=6",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "rollup": "^2.68.0",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-typescript2": "^0.31.2",
    "ts-proto": "^1.112.0",
    "typescript": "^4.6.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier",
      "prettier/@typescript-eslint",
      "prettier/react"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "release-it": {
    "gitlab": {
      "release": false
    },
    "plugins": {},
    "npm": {
      "publish": false
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --cache --fix --max-warnings=0 --ignore-path '.eslintignore'"
    ],
    "*.{json,md}": "prettier --write"
  }
}
